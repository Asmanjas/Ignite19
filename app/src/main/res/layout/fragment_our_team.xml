                                    app:civ_shadow_color="#3f51b5"
                                    app:civ_shadow_radius="5"

                                    />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="15dp"
                                    android:text="Asmanjas"
